import collections 
cd = collections.deque([2,3,4])
cd.append(5)
print ("The deque after appending at right is : ")
print (cd)
cd.appendleft(7)
print ("The deque after appending at left is : ")
print (cd)
cd.pop()
print ("The deque after deleting from right is : ")
print (cd)
cd.popleft()
print ("The deque after deleting from left is : ")
print (cd)
